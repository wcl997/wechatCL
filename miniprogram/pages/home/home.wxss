@import "./home.skeleton.wxss";
.home .home_bg{
  height: 400rpx;
  background-color: #e4393c;
}
.home .jieyu_box{
  position: relative;
  height: 150rpx;
  padding-bottom: 40rpx;
  margin: 0 20rpx;
}
.home .jieyu {
  position: absolute;
  left: 50%;
  top: 40rpx;
  transform: translateX(-50%);
  text-align: center;
}

.home .jieyu_title{
  font-size: 20rpx;
}

.home .jieyu_money{
  font-weight: bold;
  margin-top: 20rpx;
  font-size: 40rpx;
}
.home .decimal{
  font-size: 28rpx;
}
.home .zs{
  display: flex;
  padding: 0 20rpx;
}
.home .zs_item{
  flex:1;
  display: flex;
  height: 210rpx;
  align-items: center;

}

.home .zs_item_content{
  flex: 1;
  text-align: center;
}

.home .money{
  margin-top: 10rpx;
  font-weight: bold;
  font-size: 32rpx;
}
.home .sub_decimal{
  font-size: 24rpx;
}

.home .book_box_nav{
  display: flex;
  padding: 0 20rpx;
  background-color: #fff;
}
.home .nav_content{
  margin-right: auto;
  height: 80rpx;
  display: flex;
  line-height: 80rpx;
  font-size:24rpx;
  color: #999;
}
.home .b1{
  margin-left: 20rpx;
}
.home .select_box{
  display: flex;
  align-items: center;
  position: relative;
  overflow: hidden;
}
.home .point{
  width: 10rpx;
  height: 10rpx;
  background-color: #999;
  border-radius: 50%;
  margin: 0 5rpx;
}

.home .picker_box{
  position: absolute;
  right: 0;
  top: 0;
  opacity: 0;
}

.zc{
  color: rgb(36,157,98);
}
.sr{
  color:rgb(209,59,46);
}

.not-booking{
  text-align: center;
  margin-top: 100rpx;
  font-size: 28rpx;
  color: #999;
}
